    "k=int(input(\"enter the number of terms: \"))\n",
    "n1=0\n",
    "n2=1\n",
    "for n in range(0,k):\n",
    "    if (n<=1):\n",
    "        next = n\n",
    "    else:\n",
    "        next=n1+n2\n",
    "        n1=n2\n",
    "        n2=next\n",
    "    print(next)"
